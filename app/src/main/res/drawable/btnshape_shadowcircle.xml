<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval">
    <corners
            android:radius="45dp"
    />
    <gradient
            android:angle="45"
            android:centerX="35%"
            android:centerColor="#f2f2f2"
            android:startColor="#E8E8E8"
            android:endColor="#000000"
            android:type="linear"
    />
    <padding
            android:left="0dp"
            android:top="0dp"
            android:right="0dp"
            android:bottom="0dp"
    />
    <size
            android:width="270dp"
            android:height="60dp"
    />
    <stroke
            android:width="0dp"
            android:color="#878787"
    />
    <size android:height="65dp"
          android:width="65dp">

    </size>
</shape>
